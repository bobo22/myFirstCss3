/*头部*/
.header {
	display: flex;
	justify-content: space-between;
	align-items: center;
	height: 100px;
	background: url('../../img/home/img_home_bottom_bg.png') no-repeat;
}
/*.header:active {
	background-color: #eee;
}*/
.header-info,
.header-cancel {
	display: flex;
	align-items: center;
}
.header-cancel {
	position: relative;
}
.outside-circle {
	display: flex;
	align-items: center;
	width: 70px;
	height: 70px;
	border: 1px solid red;
	border-radius: 50%;
}
.inside-circle {
	margin: 0 auto;
	width: 60px;
	height: 60px;
	border-radius: 50%;
	overflow: hidden;
}
.inside-circle img {
	width: 100%;
	height: 100%;
}
.user {
	margin-left: 20px;
}
.username {
	font-size: 22px;
}
.user div:last-child {
	margin-top: 8px;
}
.half-circle {
	position: absolute;
	top: 0;
	left: -17px;
	width: 30px;
	height: 30px;
	background-color: red;
	border-radius: 50%;
	z-index: 2;
}
.cancel {
	display: flex;
	align-items: center;
	width: 70px;
	height: 30px;
	background-color: red;
	z-index: 5;
}
.cancel-img {
	width: 20px;
	height: 20px;
}
.cancel-img img {
	width: 100%;
	height: 100%;
}
.cancel-txt {
	margin-left: 6px;
}
.bg {
	width: 100%;
	height: 10px;
	background-color: #efeff4;
}
